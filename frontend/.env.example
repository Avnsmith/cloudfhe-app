# CloudFHE Frontend Environment Variables

# Smart Contract Address
REACT_APP_CLOUDFHE_ADDR=0xCe370AAb88064e94d5a3CaB3844a42A14BB0213D

# FHEVM Relayer Configuration
REACT_APP_FHEVM_PROJECT_ID=your-project-id
REACT_APP_FHEVM_API_KEY=your-api-key

# IPFS Configuration (optional)
REACT_APP_IPFS_URL=https://ipfs.infura.io:5001/api/v0
REACT_APP_IPFS_AUTH=your-ipfs-auth-token

# Cloudflare R2 Configuration
REACT_APP_R2_ENDPOINT=https://your-account-id.r2.cloudflarestorage.com
REACT_APP_R2_ACCESS_KEY_ID=your-access-key-id
REACT_APP_R2_SECRET_ACCESS_KEY=your-secret-access-key

# R2 Bucket Name
REACT_APP_R2_BUCKET=cloudfhe-bucket

# CDN Domain (optional)
REACT_APP_CDN_DOMAIN=https://cdn.example.com
